<style>
  body {
    margin: 0;
    font-family: sans-serif;
  }

  .navbar {
    background-color: black;
    color: white;
    display: flex;
    align-items: center;
    padding: 10px 20px;
  }

  .navbar-left {
    display: flex;
    align-items: center;
  }

  .navbar-left img {
    height: 30px;
    margin-right: 20px;
  }

  .search-bar {
    background-color: white;
    border-radius: 5px;
    display: flex;
    align-items: center;
    flex-grow: 1;
    margin-right: 20px;
  }

  .search-bar input {
    border: none;
    padding: 10px;
    font-size: 16px;
    flex-grow: 1;
    border-radius: 5px 0 0 5px;
  }

  .search-bar button {
    background: none;
    border: none;
    padding: 10px;
    cursor: pointer;
  }

  .navbar-right {
    display: flex;
    align-items: center;
  }

  .navbar-right a {
    color: white;
    text-decoration: none;
    margin-left: 20px;
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 14px;
  }

  .navbar-right a svg {
    width: 20px;
    height: 20px;
    margin-bottom: 5px;
  }

  .cart-badge {
    background-color: #28a745;
    /* Example green color */
    color: white;
    border-radius: 50%;
    width: 18px;
    height: 18px;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 12px;
    position: absolute;
    top: -5px;
    right: -5px;
  }

  .bottom-links {
    background-color: black;
    color: white;
    padding: 10px 20px;
    display: flex;
    gap: 20px;
  }

  .bottom-links a {
    color: white;
    text-decoration: none;
  }
</style>


